datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

model Todo {
  id          Int     @id @default(autoincrement())
  title       String
  description String
  completed   Boolean @default(false)
}
